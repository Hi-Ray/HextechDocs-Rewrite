{
  "name": "hextechdocs-rewrite",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@apollo/react-hooks": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "axios": "^0.20.0",
    "axios-hooks": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-reset-and-normalize": "^2.1.0",
    "filter-invalid-dom-props": "^2.0.0",
    "github-markdown-css": "^4.0.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "highlight.js": "^10.2.0",
    "markdown-it": "^11.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-done-right": "^4.1.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.28.0",
    "next": "^11.1.0",
    "next-html": "^0.1.1",
    "next-images": "^1.4.1",
    "react": "^16.13.1",
    "react-cookie-consent": "^5.1.3",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-image-fallback": "^8.0.0",
    "react-img-fallback": "Hi-Ray/react-img-fallback",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.1.0",
    "react-scripts": "3.4.3",
    "styled-components": "^5.2.0",
    "typescript": "~4.0.2",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "dev": "yarn legal && yarn readme && concurrently \"cross-env NODE_OPTIONS='--inspect' next dev\" \"yarn gql-codegen --watch\"",
    "start": "next start",
    "build": "yarn gql-codegen && next build",
    "prestart": "yarn legal && yarn readme && yarn build",
    "format": "prettier --write src/**/*.ts{,x}",
    "gql-codegen": "graphql-codegen --config codegen.yml",
    "legal": "markdown -f gfm legal.md > public/static/legal.html",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "readme": "markdown -f gfm README.md > public/static/readme.html"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.9",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-graphql-files-modules": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/jest": "^26.0.13",
    "@types/next": "^9.0.0",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "markdown-to-html": "^0.0.13",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "wait-on": "^5.2.0"
  }
}
